<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.darkayy.aueraaetas.emp"
    android:background="@drawable/backgroundetc">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Biete an:"
        android:id="@+id/txtBieteAn"
        android:layout_marginTop="117dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textColor="#f1fcead3" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/editText8"
        android:hint="Rohstoffname"
        android:textColor="#f1fcead3"
        android:layout_below="@+id/txtBieteAn"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/editText9"
        android:hint="Menge"
        android:textColor="#f1fcead3"
        android:layout_alignTop="@+id/editText8"
        android:layout_toRightOf="@+id/editText8"
        android:maxWidth="100dp"
        android:layout_marginRight="80dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Möchte haben:"
        android:id="@+id/txtMoechteHaben"
        android:layout_marginTop="20dp"
        android:textColor="#f1fcead3"
        android:layout_below="@+id/editText8"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/editText10"
        android:hint="Rohstoffname"
        android:textColor="#f1fcead3"
        android:layout_below="@+id/txtMoechteHaben"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/editText11"
        android:hint="Menge"
        android:textColor="#f1fcead3"
        android:maxWidth="100dp"
        android:layout_below="@+id/txtMoechteHaben"
        android:layout_toRightOf="@+id/editText10"
        android:layout_toEndOf="@+id/editText10"
        android:layout_marginRight="80dp" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Transaktion veröffentlichen"
        android:id="@+id/btntransaktion"
        android:layout_below="@+id/editText11"
        android:layout_alignRight="@+id/editText11"
        android:layout_alignEnd="@+id/editText11"
        android:textColor="#471d01"
        android:background="#aefad7a2" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Meine Angebote:"
        android:id="@+id/txtMeineAngebote"
        android:layout_below="@+id/btntransaktion"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="25dp"
        android:textColor="#fdf4ef" />

    <ScrollView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/scrollView4"
        android:layout_below="@+id/txtMeineAngebote"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_alignRight="@+id/btntransaktion"
        android:layout_alignEnd="@+id/btntransaktion" >

        <TableLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="Rohstoff angebot"
                    android:id="@+id/textView59"
                    android:layout_column="1"
                    android:textColor="#f1fcead3" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="Rohstoff ford."
                    android:id="@+id/textView60"
                    android:layout_column="2"
                    android:textColor="#f1fcead3"
                    android:layout_marginLeft="10dp" />
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="Rohstoff angebot"
                    android:id="@+id/textView61"
                    android:layout_column="1"
                    android:textColor="#f1fcead3" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="Rohstoff ford."
                    android:id="@+id/textView62"
                    android:layout_column="2"
                    android:textColor="#f1fcead3"
                    android:layout_marginLeft="10dp" />
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="Rohstoff angebot"
                    android:id="@+id/textView63"
                    android:layout_column="1"
                    android:textColor="#f1fcead3" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="Rohstoff ford."
                    android:id="@+id/textView64"
                    android:layout_column="2"
                    android:textColor="#f1fcead3"
                    android:layout_marginLeft="10dp" />
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="Rohstoff angebot"
                    android:id="@+id/textView65"
                    android:layout_column="1"
                    android:textColor="#f1fcead3" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="Rohstoff ford."
                    android:id="@+id/textView66"
                    android:layout_column="2"
                    android:textColor="#f1fcead3"
                    android:layout_marginLeft="10dp" />
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="Rohstoff angebot"
                    android:id="@+id/textView67"
                    android:layout_column="1"
                    android:textColor="#f1fcead3" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="Rohstoff ford."
                    android:id="@+id/textView68"
                    android:layout_column="2"
                    android:textColor="#f1fcead3"
                    android:layout_marginLeft="10dp" />
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="Rohstoff angebot"
                    android:id="@+id/textView69"
                    android:layout_column="1"
                    android:textColor="#f1fcead3" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="Rohstoff ford."
                    android:id="@+id/textView70"
                    android:layout_column="2"
                    android:textColor="#f1fcead3"
                    android:layout_marginLeft="10dp" />
            </TableRow>
        </TableLayout>
    </ScrollView>

</RelativeLayout>
